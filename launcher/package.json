{
  "name": "rwe-launcher",
  "version": "0.1.0",
  "description": "Launcher application for Robot War Engine",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build:main": "webpack --config webpack.main.config.js",
    "build:master": "webpack --config webpack.master.config.js",
    "build:renderer": "webpack --config webpack.renderer.config.js",
    "build": "npm run build:master && npm run build:main && npm run build:renderer",
    "start": "npm run build:main && cross-env RWE_LAUNCHER_IS_DEV=1 electron ./dist/main.js",
    "server": "webpack-dev-server --config webpack.renderer.config.js",
    "master-server": "npm run build:master && node dist/master-server.js",
    "clean": "rm -rf ./dist",
    "package": "npm run build && electron-packager dist --arch=x64 --overwrite --asar",
    "lint": "npx eslint . --ext=.ts --ext=.tsx",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook"
  },
  "author": "Michael Heasell",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/react": "^5.2.1",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/storybook__react": "^4.0.2",
    "@types/yargs": "^13.0.2",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "electron": "^6.0.9",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.0.6",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "prettier": "^1.18.2",
    "style-loader": "^1.0.0",
    "typescript": "^3.6.3",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "7zip": "0.0.6",
    "@hot-loader/react-dom": "^16.9.0",
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "@types/react-window": "^1.8.1",
    "bufferutil": "^4.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "react-redux": "^7.1.1",
    "react-window": "^1.8.5",
    "redux": "^4.0.4",
    "redux-observable": "^1.1.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.3",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "utf-8-validate": "^5.0.2",
    "yargs": "^14.0.0"
  }
}
