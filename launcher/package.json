{
  "name": "rwe-launcher",
  "version": "0.1.0",
  "description": "Launcher application for Robot War Engine",
  "private": true,
  "main": "main.js",
  "scripts": {
    "build:main": "webpack --config webpack.main.config.js",
    "build:master": "webpack --config webpack.master.config.js",
    "build:renderer": "webpack --config webpack.renderer.config.js",
    "build": "npm run build:master && npm run build:main && npm run build:renderer",
    "start": "npm run build:main && cross-env RWE_LAUNCHER_IS_DEV=1 electron ./dist/main.js",
    "server": "webpack-dev-server --config webpack.renderer.config.js",
    "master-server": "npm run build:master && node dist/master-server.js",
    "clean": "rm -rf ./dist",
    "rebuild": "electron-rebuild --arch=x64 --only registry-js",
    "package": "npm run rebuild && npm run build && electron-packager dist --arch=x64 --overwrite --asar",
    "lint": "npx eslint . --ext=.ts --ext=.tsx",
    "tsc": "tsc --noEmit",
    "storybook": "start-storybook"
  },
  "author": "Michael Heasell",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@storybook/react": "^5.2.8",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/yargs": "^13.0.4",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.4.0",
    "electron": "^6.1.7",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.1.1",
    "electron-rebuild": "^1.8.8",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^4.3.0",
    "node-loader": "^0.6.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "7zip": "0.0.6",
    "@hot-loader/react-dom": "^16.11.0",
    "@material-ui/core": "^4.8.0",
    "@material-ui/icons": "^4.5.1",
    "@types/react-window": "^1.8.1",
    "bufferutil": "^4.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "react-window": "^1.8.5",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "registry-js": "^1.9.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "utf-8-validate": "^5.0.2",
    "yargs": "^14.2.2"
  }
}
